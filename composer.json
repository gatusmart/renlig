{
    "name": "laraone/phoenix",
    "description": "Laraone Phoenix, backend for Laraone CMS",
    "keywords": ["laraone", "cms", "responsive", "web-design", "themes", "php", "javascript", "components"],
    "type": "project",
    "homepage": "https://laraone.com",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.6.3",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "2.5.0",
        "laraone/themes": "^1.0.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "mews/purifier": "2.1.4",
        "santigarcor/laratrust": "5.0.*",
        "silber/page-cache": "^1.0",
        "tightenco/ziggy": "0.6.9"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
